var maxArea = function(height) {
    let max = 0;
    for(let i = 0; i < height.length; i++) {
        for(let j = i + 1; j < height.length; j++) {
            const area = Math.min(height[i], height[j]) * (j - i);
            if(area > max) {
                max = area;
            }
        }
    }
    
    return max;
    
};

//O(n^2) time, O(1) space
